<% if (this.collection.length > 0) { %>
  <ul class='unstyled'>
    <% this.collection.forEach(function (credential) { %>
    <li>
      <h4>
        <a data-action='editCredential'
          data-target-id='<%= credential.id %>'>

          <%= credential.get('name') %>
        </a>
      </h4>
    </li>
    <% }) %>
  </ul>
<% } else { %>
  <h2>No Credentials. Would you like to
    <a href='/credentials/add'>add one?</a>
  </h2>
<% } %>
